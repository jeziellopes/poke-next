    "lint:fix": "eslint --fix src",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "next": "latest",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
