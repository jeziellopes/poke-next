{
  "name": "poke-next",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:ci": "CI=true npm test -- --coverage",
